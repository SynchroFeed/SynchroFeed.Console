<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <BuildDependsOn>$(BuildDependsOn);AfterBuildMigrated</BuildDependsOn>
    <Authors>Bob Vandehey</Authors>
    <PackageId>SynchroFeed.Console</PackageId>
    <PackageLicenseUrl>
    </PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/SynchroFeed/SynchroFeed.Console</PackageProjectUrl>
    <PackageTags>nuget chocolately sync feed package catalog</PackageTags>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <PropertyGroup>
    <StartupObject>SynchroFeed.Console.Program</StartupObject>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>synchrofeed.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="..\CompanyAssemblyInfo.cs" Link="Properties\CompanyAssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\SynchroFeed.png">
      <Pack>True</Pack>
      <PackagePath>
      </PackagePath>
    </None>
    <None Include="App.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Content Include="synchrofeed.ico" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.CommandLineUtils" Version="1.1.1" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="3.1.3" />
    <PackageReference Include="NLog.Extensions.Logging" Version="1.6.2" />
    <PackageReference Include="SynchroFeed.Action.Process" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Action.Sync" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.ActionObserver.WebPost" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Command.ApplicationIs64bit" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Command.Catalog" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Command.ConfigReview" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Command.Log4netReview" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Command.NugetContainsSupportFiles" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Command.VersioningCheck" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Repository.Directory" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Repository.Npm" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Repository.Nuget" Version="0.0.104" />
    <PackageReference Include="SynchroFeed.Repository.Proget" Version="0.0.104" />
  </ItemGroup>
  <ItemGroup>
    <None Update="App.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="NLog.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>